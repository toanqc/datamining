<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>SelfMiningServlet</servlet-name>
    <servlet-class>mum.bigdata.data.mining.SelfMiningServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>GroupMiningServlet</servlet-name>
    <servlet-class>mum.bigdata.data.mining.GroupMiningServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelfMiningServlet</servlet-name>
    <url-pattern>/self.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GroupMiningServlet</servlet-name>
    <url-pattern>/group.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>